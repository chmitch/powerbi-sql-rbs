  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    
    
    <add key="ida:ClientId" value="[aad webapp id]" />
    <add key="ida:Tenant" value="[aad tenant name].onmicrosoft.com" />
    <add key="ida:AADInstance" value="https://login.microsoftonline.com/{0}" />
    <add key="ida:PostLogoutRedirectUri" value="http://localhost:4279/" />
    <add key="ida:ClientSecret" value="[aad webapp client secret]" />
    
    <add key="authorityUrl" value="https://login.microsoftonline.com/[aad tenant name]" />
    
    <add key="sqlApplicationId" value="[aad webapp id]" />
    <add key="sqlResourceUrl" value="https://database.windows.net/" />

    <add key="pbiApplicationId" value="[aad native app id]" />
    <add key="pbiResourceUrl" value="https://analysis.windows.net/powerbi/api" />
    <add key="workspaceId" value="[power bi workspaceid]" />
    <add key="reportId" value="[power bi reportid]" />
    <add key="pbiUsername" value="[power be pro user]" />
    <add key="apiUrl" value="https://api.powerbi.com/" />
    <add key="embedUrlBase" value="https://app.powerbi.com/" />
    <add key="pbiPassword" value="[power bi pro user password]" />
  </appSettings>
  <system.identityModel>
    <identityConfiguration saveBootstrapContext="true" />
  </system.identityModel>
